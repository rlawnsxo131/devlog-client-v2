{
    "presets": [
        ["@babel/preset-react", {
            "runtime": "automatic"
        }],
        [
            "@babel/preset-env",
            { "targets": { "browsers": ["last 2 versions", ">= 5% in KR"] } }
        ],
        "@babel/preset-typescript"
    ],      
    "plugins": [
        "@loadable/babel-plugin", 
        "@babel/plugin-syntax-dynamic-import"
    ]
}